@model IEnumerable<MVCVooraadBeheer.Models.LocationMagazineTitleWarning>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Active)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActiveTo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.value)
        </th>
        <th>
